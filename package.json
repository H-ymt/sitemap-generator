{
  "name": "sitemap-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:cf": "next build && npx @cloudflare/next-on-pages",
    "deploy": "npm run build:cf && wrangler deploy",
    "workers:build": "cd workers && pnpm install && pnpm build",
    "workers:deploy": "cd workers && wrangler deploy",
    "workers:dev": "cd workers && wrangler dev --port 8787 --local",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.4.0",
    "@playwright/test": "^1.53.2",
    "cheerio": "^1.1.0",
    "jotai": "^2.12.5",
    "next": "^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.6",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "typescript": "^5.0.0",
    "wrangler": "^4.22.0"
  }
}
